(setq vc-follow-symlinks)

(setq load-path (cons (expand-file-name "~/elisp") load-path))

(require 'yaml-mode)
(add-to-list 'auto-mode-alist '("\\.yml$" . yaml-mode))

(put 'upcase-region 'disabled nil)

(setq vc-handled-backends nil)

