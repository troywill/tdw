;; http://emacswiki.org/emacs/ElispCookbook
(defun write-string-to-file (string file)
  (interactive "sEnter the string: \nFFile to save to: ")
  (with-temp-buffer
    (insert string)
    (when (file-writable-p file)
      (append-to-file (point-min) (point-max) file)
      )
    )
  )

